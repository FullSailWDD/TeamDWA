<div ng-app='app'>
<a href="/logout" class="btn btn-default btn-sm">Logout</a>

<h1>courses</h1>
    <form action="/addCourse" method="post">
        <div class="form-group">
            <label>Course Title</label>
            <input type="text" class="form-control" name="courseTitle">
        </div>
        <div class="form-group">
            <label>Description</label>
            <textarea class="form-control" name="courseDes">
            </textarea>
        </div>
        <button type="submit" class="btn btn-warning btn-lg">Add Course</button>
    </form>

    <div ng-controller="courseGenerator">
        <courses data-payload='courseGeneratorData' data-callback=''></courses>
    </div>
</div>